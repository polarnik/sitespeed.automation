{
  "extends": "/config/default.json",

  "browsertime": {
    "browser": "chrome",
    "chrome": {
      "args": [
        "--user-data-dir=/tmp/chrome-user-data-dir",
        "--enable-logging",
        "--enable-sandbox-logging",
        "--log-level=0"
      ],
      "_traceCategories_Web_developer": "blink,cc,netlog,renderer.scheduler,sequence_manager,toplevel,v8",
      "_traceCategories_Input_latency": "benchmark,input,evdev,renderer.scheduler,sequence_manager,toplevel",
      "_traceCategories_Rendering": "benchmark,blink,cc,gpu,toplevel,viz",
      "_traceCategories_UI_Rendering": "benchmark,cc,gpu,input,toplevel,ui,views,viz",
      "_traceCategories_Javascript_and_rendering": "blink,cc,gpu,renderer.scheduler,sequence_manager,v8,toplevel,viz",
      "_traceCategories_Frame_Viewer": "blink,cc,gpu,renderer.scheduler,sequence_manager,v8,toplevel,disabled-by-default-blink.invalidation,disabled-by-default-cc.debug,disabled-by-default-cc.debug.picture,disabled-by-default-cc.debug.display_items",
      "_traceCategories_Chrome_developer_overall": "benchmark,toplevel,ipc,base,ui,v8,renderer,blink,blink_gc,mojom,latency,latencyInfo,renderer_host,cc,memory,dwrite,fonts,browser,ServiceWorker,disabled-by-default-v8.gc,disabled-by-default-file,disabled-by-default-blink_gc,disabled-by-default-lifecycles,disabled-by-default-renderer.scheduler,disabled-by-default-system_stats,disabled-by-default-cpu_profiler,passwords,sql,disabled-by-default-user_action_samples,startup,disk_cache",
      "_traceCategories_Chrome_developer_navigation": "benchmark,toplevel,ipc,base,browser,navigation,omnibox,ui,shutdown,safe_browsing,Java,EarlyJava,loading,startup,mojom,renderer_host,disabled-by-default-system_stats,disabled-by-default-cpu_profiler,dwrite,fonts,ServiceWorker,passwords,disabled-by-default-file,sql,disabled-by-default-user_action_samples,disk_cache",
      "traceCategories": "blink,cc",
      "enableTraceScreenshots": false,
      "enableChromeDriverLog": true,
      "enableVerboseChromeDriverLog": true,
      "timeline": true,
      "collectPerfLog": true,
      "collectNetLog": true,
      "netLogCaptureMode": "Everything",
      "collectConsoleLog": true,
      "cleanUserDataDir": false,
      "CPUThrottlingRate": 1,
      "includeResponseBodies": false
    },
    "limitedRunData": false,
    "connectivity": {
      "engine": "external",
      "profile": "cable"
    },
    "viewPort": "3456Ã—2234",
    "appendToUserAgent": " crawler/1.0",
    "visualMetrics": true,
    "video": true,
    "screenshotLCP": true,
    "screenshotLS": true,
    "cpu": true,
    "utc": true,
    "xvfb": true
  },
  "useHash": true,
  "cpu": true,
  "utc": true,
  "plugins": {
    "list": true,
    "remove": ["/gpsi/lib/index.js", "/gpsi", "gpsi"],
    "add": ["/lighthouse/index.js"]
  },
  "processStartTime": true,
  "prettyPrint": false
}